{
  "name": "mingcare-intranet",
  "version": "1.0.5",
  "description": "明家居家護理服務 Intranet 系統",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build",
    "build:hostinger": "rm -rf .next && NODE_ENV=production next build",
    "build:github": "rm -rf .next && cp next.config.github-pages.js next.config.js && next build && git checkout next.config.js",
    "deploy:hostinger": "npm run build:hostinger && npm run start",
    "deploy": "npm run build:github",
    "clean": "rm -rf .next out node_modules/.cache"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.77",
    "@nodelib/fs.walk": "^3.0.1",
    "@supabase/ssr": "^0.0.10",
    "@supabase/supabase-js": "^2.38.0",
    "autoprefixer": "^10.4.0",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.10.3",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "jszip": "^3.10.1",
    "next": "^14.0.0",
    "pdfmake": "^0.2.20",
    "postcss": "^8.4.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^3.3.0"
  },
  "devDependencies": {
    "@types/html2pdf.js": "^0.10.0",
    "@types/jspdf": "^1.3.3",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "typescript": "^5.0.0"
  }
}
